[user]
    email = kjemmett@gmail.com
    name = Kevin Emmett
[core]
    excludesfile = ~/.gitignore
    attributesfile = ~/.gitattributes
[push]
    default = simple
[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow 
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[merge]
    # Include summaries of merged commits in newly created merge commit messages
    log = true
